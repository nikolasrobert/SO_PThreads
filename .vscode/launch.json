{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Paralelo (GDB)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/busca_paralela.exe",
      "args": ["8", "100", "100"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/MinGW/bin/gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
